{
  "name": "@oxheadalpha/fa2-contracts",
  "version": "3.1.0",
  "description": "Modular implementation of FA2 Tezos smart contracts in LIGO language",
  "repository": "https://github.com/oxheadalpha/nft-tutorial.git",
  "keywords": [
    "tezos",
    "smart contracts",
    "NFT",
    "FA2",
    "TZIP-12",
    "TZIP-21"
  ],
  "license": "MIT",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*",
    "ligo/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "clear": "yarn rimraf dist",
    "build": "tsc -p . && chmod +x ./dist/export-ligo.js",
    "prepare": "yarn clear && yarn build",
    "test-contracts": "jest --runInBand",
    "test": "(yarn start-sandbox && yarn test-contracts) ; yarn kill-sandbox"
  },
  "bin": {
    "export-ligo": "./dist/export-ligo.js"
  },
  "devDependencies": {
    "@jest/types": "^27.4.2",
    "@taquito/taquito": "^11.0.2",
    "@taquito/signer": "^11.0.2",
    "@oxheadalpha/fa2-interfaces": "^3.1.0",
    "@oxheadalpha/tezos-tools": "^3.1.0",
    "@types/jest": "^27.0.3",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "bignumber": "^1.1.0",
    "fs": "^0.0.1-security",
    "kleur": "^4.1.4",
    "path": "^0.12.7",
    "process": "^0.11.10"
  },
  "gitHead": "1c80cc3e323b7dcadaf1ff8c3b80ebfe8d407369"
}
