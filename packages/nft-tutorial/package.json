    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@oxheadalpha/fa2-interfaces": "^2.0.8",
    "@oxheadalpha/tezos-tools": "^2.0.8",
    "@taquito/signer": "^11.0.2",
