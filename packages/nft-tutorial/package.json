{
  "name": "@oxheadalpha/nft-tutorial",
  "version": "2.0.8",
  "description": "CLI tool and tutorial on how to create FA2 non-fungible tokens on Tezos",
  "repository": "https://github.com/oxheadalpha/nft-tutorial.git",
  "homepage": "https://github.com/oxheadalpha/nft-tutorial/packages/nft-tutorial#README.md",
  "keywords": [
    "tezos",
    "smart contracts",
    "NFT",
    "FA2",
    "TZIP-12",
    "TZIP-21"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsc -p . && chmod +x dist/tznft.js && yarn compile-contract",
    "prepare": "yarn build",
    "compile-contract": "ts-node ./src/compile-contract.ts",
    "tznft": "./dist/tznft.js"
  },
  "bin": {
    "tznft": "./dist/tznft.js"
  },
  "files": [
    "dist/*",
    "ligo/*",
    "tznft.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.3",
    "@types/configstore": "^5.0.1",
    "@types/node": "^17.0.0",
